/*  Used in the VIDIOC_QUERYCTRL ioctl for querying controls */
v4l2_queryctrl :: struct {
    id:            u32;
    type:          u32; /* enum v4l2_ctrl_type */
    name:          [32] u8; /* Whatever */
    minimum:       s32; /* Note signedness */
    maximum:       s32;
    step:          s32;
    default_value: s32;
    flags:         u32;
    reserved:      [2] u32;
}

/*  Used in the VIDIOC_QUERY_EXT_CTRL ioctl for querying extended controls */
v4l2_query_ext_ctrl :: struct {
    id:            u32;
    type:          u32;
    name:          [32] u8;
    minimum:       s64;
    maximum:       s64;
    step:          u64;
    default_value: s64;
    flags:         u32;
    elem_size:     u32;
    elems:         u32;
    nr_of_dims:    u32;
    dims:          [4] u32;
    reserved:      [32] u32;
}

/*  Used in the VIDIOC_QUERYMENU ioctl for querying menu items */
v4l2_querymenu :: struct {
    id:       u32;
    index:    u32;
    union {
        name:  [32] u8; /* Whatever */
        value: s64;
    }

    reserved: u32;
} #no_padding

/*
*	T U N I N G
*/
v4l2_tuner :: struct {
    index:      u32;
    name:       [32] u8;
    type:       u32; /* enum v4l2_tuner_type */
    capability: u32;
    rangelow:   u32;
    rangehigh:  u32;
    rxsubchans: u32;
    audmode:    u32;
    signal:     s32;
    afc:        s32;
    reserved:   [4] u32;
}

v4l2_modulator :: struct {
    index:      u32;
    name:       [32] u8;
    capability: u32;
    rangelow:   u32;
    rangehigh:  u32;
    txsubchans: u32;
    type:       u32; /* enum v4l2_tuner_type */
    reserved:   [3] u32;
}

v4l2_frequency :: struct {
    tuner:     u32;
    type:      u32; /* enum v4l2_tuner_type */
    frequency: u32;
    reserved:  [8] u32;
}

v4l2_frequency_band :: struct {
    tuner:      u32;
    type:       u32; /* enum v4l2_tuner_type */
    index:      u32;
    capability: u32;
    rangelow:   u32;
    rangehigh:  u32;
    modulation: u32;
    reserved:   [9] u32;
}

v4l2_hw_freq_seek :: struct {
    tuner:       u32;
    type:        u32; /* enum v4l2_tuner_type */
    seek_upward: u32;
    wrap_around: u32;
    spacing:     u32;
    rangelow:    u32;
    rangehigh:   u32;
    reserved:    [5] u32;
}

/*
*	R D S
*/
v4l2_rds_data :: struct {
    lsb:   u8;
    msb:   u8;
    block: u8;
}

/*
*	A U D I O
*/
v4l2_audio :: struct {
    index:      u32;
    name:       [32] u8;
    capability: u32;
    mode:       u32;
    reserved:   [2] u32;
}

v4l2_audioout :: struct {
    index:      u32;
    name:       [32] u8;
    capability: u32;
    mode:       u32;
    reserved:   [2] u32;
}

v4l2_enc_idx_entry :: struct {
    offset:   u64;
    pts:      u64;
    length:   u32;
    flags:    u32;
    reserved: [2] u32;
}

v4l2_enc_idx :: struct {
    entries:     u32;
    entries_cap: u32;
    reserved:    [4] u32;
    entry:       [64] v4l2_enc_idx_entry;
}

v4l2_encoder_cmd :: struct {
    cmd:   u32;
    flags: u32;
    union {
        raw: struct {
            data: [8] u32;
        };
    }
}

/* The structure must be zeroed before use by the application
This ensures it can be extended safely in the future. */
v4l2_decoder_cmd :: struct {
    cmd:   u32;
    flags: u32;
    union {
        stop:  struct {
            pts: u64;
        };

        start: struct {
            /* 0 or 1000 specifies normal speed,
            1 specifies forward single stepping,
            -1 specifies backward single stepping,
            >1: playback at speed/1000 of the normal speed,
            <-1: reverse playback at (-speed/1000) of the normal speed. */
            speed:  s32;
            format: u32;
        };

        raw:   struct {
            data: [16] u32;
        };
    }
}

/* Raw VBI */
v4l2_vbi_format :: struct {
    sampling_rate:    u32; /* in 1 Hz */
    offset:           u32;
    samples_per_line: u32;
    sample_format:    u32; /* V4L2_PIX_FMT_* */
    start:            [2] s32;
    count:            [2] u32;
    flags:            u32; /* V4L2_VBI_* */
    reserved:         [2] u32; /* must be zero */
}

/* Sliced VBI
*
*    This implements is a proposal V4L2 API to allow SLICED VBI
* required for some hardware encoders. It should change without
* notice in the definitive implementation.
*/
v4l2_sliced_vbi_format :: struct {
    service_set:   u16;

    /* service_lines[0][...] specifies lines 0-23 (1-23 used) of the first field
    service_lines[1][...] specifies lines 0-23 (1-23 used) of the second field
    (equals frame lines 313-336 for 625 line video
    standards, 263-286 for 525 line standards) */
    service_lines: [2] [24] u16;
    io_size:       u32;
    reserved:      [2] u32; /* must be zero */
}

v4l2_sliced_vbi_cap :: struct {
    service_set:   u16;

    /* service_lines[0][...] specifies lines 0-23 (1-23 used) of the first field
    service_lines[1][...] specifies lines 0-23 (1-23 used) of the second field
    (equals frame lines 313-336 for 625 line video
    standards, 263-286 for 525 line standards) */
    service_lines: [2] [24] u16;
    type:          u32; /* enum v4l2_buf_type */
    reserved:      [3] u32; /* must be 0 */
}

v4l2_sliced_vbi_data :: struct {
    id:       u32;
    field:    u32; /* 0: first field, 1: second field */
    line:     u32; /* 1-23 */
    reserved: u32; /* must be 0 */
    data:     [48] u8;
}

v4l2_mpeg_vbi_itv0_line :: struct {
    id:   u8; /* One of V4L2_MPEG_VBI_IVTV_* above */
    data: [42] u8; /* Sliced VBI data for the line */
}

v4l2_mpeg_vbi_itv0 :: struct {
    linemask: [2] u32; /* Bitmasks of VBI service lines present */
    line:     [35] v4l2_mpeg_vbi_itv0_line;
} #no_padding

v4l2_mpeg_vbi_ITV0 :: struct {
    line: [36] v4l2_mpeg_vbi_itv0_line;
}

v4l2_mpeg_vbi_fmt_ivtv :: struct {
    magic: [4] u8;
    union {
        itv0: v4l2_mpeg_vbi_itv0;
        ITV0: v4l2_mpeg_vbi_ITV0;
    }
}

/**
* struct v4l2_plane_pix_format - additional, per-plane format definition
* @sizeimage:		maximum size in bytes required for data, for which
*			this plane will be used
* @bytesperline:	distance in bytes between the leftmost pixels in two
*			adjacent lines
* @reserved:		drivers and applications must zero this array
*/
v4l2_plane_pix_format :: struct {
    sizeimage:    u32;
    bytesperline: u32;
    reserved:     [6] u16;
}

/**
* struct v4l2_pix_format_mplane - multiplanar format definition
* @width:		image width in pixels
* @height:		image height in pixels
* @pixelformat:	little endian four character code (fourcc)
* @field:		enum v4l2_field; field order (for interlaced video)
* @colorspace:		enum v4l2_colorspace; supplemental to pixelformat
* @plane_fmt:		per-plane information
* @num_planes:		number of planes for this format
* @flags:		format flags (V4L2_PIX_FMT_FLAG_*)
* @ycbcr_enc:		enum v4l2_ycbcr_encoding, Y'CbCr encoding
* @hsv_enc:		enum v4l2_hsv_encoding, HSV encoding
* @quantization:	enum v4l2_quantization, colorspace quantization
* @xfer_func:		enum v4l2_xfer_func, colorspace transfer function
* @reserved:		drivers and applications must zero this array
*/
v4l2_pix_format_mplane :: struct {
    width:        u32;
    height:       u32;
    pixelformat:  u32;
    field:        u32;
    colorspace:   u32;

    plane_fmt:    [8] v4l2_plane_pix_format #align 1;
    num_planes:   u8;
    flags:        u8;
    union {
        ycbcr_enc: u8;
        hsv_enc:   u8;
    }

    quantization: u8;
    xfer_func:    u8;
    reserved:     [7] u8;
}

/**
* struct v4l2_sdr_format - SDR format definition
* @pixelformat:	little endian four character code (fourcc)
* @buffersize:		maximum size in bytes required for data
* @reserved:		drivers and applications must zero this array
*/
v4l2_sdr_format :: struct {
    pixelformat: u32;
    buffersize:  u32;
    reserved:    [24] u8;
}

/**
* struct v4l2_meta_format - metadata format definition
* @dataformat:		little endian four character code (fourcc)
* @buffersize:		maximum size in bytes required for data
*/
v4l2_meta_format :: struct {
    dataformat: u32;
    buffersize: u32;
}

/**
* struct v4l2_format - stream data format
* @type:	enum v4l2_buf_type; type of the data stream
* @pix:	definition of an image format
* @pix_mp:	definition of a multiplanar image format
* @win:	definition of an overlaid image
* @vbi:	raw VBI capture or output parameters
* @sliced:	sliced VBI capture or output parameters
* @raw_data:	placeholder for future extensions and custom formats
* @fmt:	union of @pix, @pix_mp, @win, @vbi, @sliced, @sdr, @meta
*		and @raw_data
*/
v4l2_format :: struct {
    type: v4l2_buf_type;
    fmt:  union {
        pix:      v4l2_pix_format; /* V4L2_BUF_TYPE_VIDEO_CAPTURE */
        pix_mp:   v4l2_pix_format_mplane; /* V4L2_BUF_TYPE_VIDEO_CAPTURE_MPLANE */
        win:      v4l2_window; /* V4L2_BUF_TYPE_VIDEO_OVERLAY */
        vbi:      v4l2_vbi_format; /* V4L2_BUF_TYPE_VBI_CAPTURE */
        sliced:   v4l2_sliced_vbi_format; /* V4L2_BUF_TYPE_SLICED_VBI_CAPTURE */
        sdr:      v4l2_sdr_format; /* V4L2_BUF_TYPE_SDR_CAPTURE */
        meta:     v4l2_meta_format; /* V4L2_BUF_TYPE_META_CAPTURE */
        raw_data: [200] u8; /* user-defined */
    };
}

/*	Stream type-dependent parameters
*/
v4l2_streamparm :: struct {
    type: u32; /* enum v4l2_buf_type */
    parm: union {
        capture:  v4l2_captureparm;
        output:   v4l2_outputparm;
        raw_data: [200] u8; /* user-defined */
    };
}

/* Payload for V4L2_EVENT_VSYNC */
v4l2_event_vsync :: struct {
    /* Can be V4L2_FIELD_ANY, _NONE, _TOP or _BOTTOM */
    field: u8;
}

v4l2_event_ctrl :: struct {
    changes:       u32;
    type:          u32;
    union {
        value:   s32;
        value64: s64;
    }

    flags:         u32;
    minimum:       s32;
    maximum:       s32;
    step:          s32;
    default_value: s32;
}

v4l2_event_frame_sync :: struct {
    frame_sequence: u32;
}

v4l2_event_src_change :: struct {
    changes: u32;
}

/**
* struct v4l2_event_motion_det - motion detection event
* @flags:             if V4L2_EVENT_MD_FL_HAVE_FRAME_SEQ is set, then the
*                     frame_sequence field is valid.
* @frame_sequence:    the frame sequence number associated with this event.
* @region_mask:       which regions detected motion.
*/
v4l2_event_motion_det :: struct {
    flags:          u32;
    frame_sequence: u32;
    region_mask:    u32;
}

v4l2_event :: struct {
    type:      u32;
    u:         union {
        vsync:      v4l2_event_vsync;
        ctrl:       v4l2_event_ctrl;
        frame_sync: v4l2_event_frame_sync;
        src_change: v4l2_event_src_change;
        motion_det: v4l2_event_motion_det;
        data:       [64] u8;
    };

    pending:   u32;
    sequence:  u32;
    timestamp: timespec;
    id:        u32;
    reserved:  [8] u32;
}

v4l2_event_subscription :: struct {
    type:     u32;
    id:       u32;
    flags:    u32;
    reserved: [5] u32;
}

v4l2_dbg_match :: struct {
    type: u32; /* Match type */
    union {
        addr: u32;
        name: [32] u8;
    }
}

v4l2_dbg_register :: struct {
    match: v4l2_dbg_match;
    size:  u32; /* register size in bytes */
    reg:   u64;
    val:   u64;
}

/* VIDIOC_DBG_G_CHIP_INFO */
v4l2_dbg_chip_info :: struct {
    match:    v4l2_dbg_match;
    name:     [32] u8;
    flags:    u32;
    reserved: [32] u32;
}

/**
* struct v4l2_create_buffers - VIDIOC_CREATE_BUFS argument
* @index:	on return, index of the first created buffer
* @count:	entry: number of requested buffers,
*		return: number of created buffers
* @memory:	enum v4l2_memory; buffer memory type
* @format:	frame format, for which buffers are requested
* @capabilities: capabilities of this buffer type.
* @flags:	additional buffer management attributes (ignored unless the
*		queue has V4L2_BUF_CAP_SUPPORTS_MMAP_CACHE_HINTS capability
*		and configured for MMAP streaming I/O).
* @max_num_buffers: if V4L2_BUF_CAP_SUPPORTS_MAX_NUM_BUFFERS capability flag is set
*		this field indicate the maximum possible number of buffers
*		for this queue.
* @reserved:	future extensions
*/
v4l2_create_buffers :: struct {
    index:           u32;
    count:           u32;
    memory:          u32;
    format:          v4l2_format;
    capabilities:    u32;
    flags:           u32;
    max_num_buffers: u32;
    reserved:        [5] u32;
}

#scope_file

